(define mn 0.000001)
(define (my_abs x)
  (if (> x 0) x
    (- x)))
(define (nxt_guess guess x)
  (/ (+ (/ x (* guess guess)) (* 2 guess)) 3))
(define (tri_good_enough guess x) 
  (< (my_abs (- guess (/ x guess guess))) mn))
(define (tri_sqrt_iter guess x)
  (if (tri_good_enough guess x) guess
    (tri_sqrt_iter (nxt_guess guess x) x)))
(define (tri_sqrt x)
  (tri_sqrt_iter 1.0 x))
(tri_sqrt 129.0) 
